<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:max="http://www.ibm.com/maximo">
   <soapenv:Header/>
   <soapenv:Body>
      <max:CreateMXPERSON>
         <max:MXPERSONSet>
         		<max:PERSON action="AddChange" transLanguage="EN">
     			<max:PERSONID>${#TestCase#personID}</max:PERSONID>
     			<max:FIRSTNAME>${#TestCase#firstName}</max:FIRSTNAME>
     			<max:LASTNAME>${#TestCase#lastName}</max:LASTNAME>
      		</max:PERSON>
      	</max:MXPERSONSet>
      </max:CreateMXPERSON>
   </soapenv:Body>
</soapenv:Envelope>                     